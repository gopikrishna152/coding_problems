import java.util.*;

public class prority_queue {
    public static void main(String[] args) { 
       //PriorityQueue<Integer> pq = new PriorityQueue<>();   //ascending order ny default
        PriorityQueue<Integer> pq = new PriorityQueue<>(Comparator.reverseOrder());//descending order
        pq.add(3);
        pq.add(4);
        pq.add(1);
        pq.add(10);
        while (!pq.isEmpty()) {
        System.out.println(pq.peek());
        pq.remove();
        }

        
} 
}
